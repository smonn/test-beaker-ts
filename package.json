    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.15",
    "beaker-ts": "^0.0.20",
    "jest": "^29.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "algosdk": "^1.20.0"
  }
}
